{
  "name": "eslint-plugin-import-x",
  "version": "0.0.0",
  "description": "Import with sanity.",
  "engines": {
    "node": ">=4"
  },
  "packageManager": "yarn@1.22.19",
  "main": "lib/index.js",
  "directories": {
    "test": "tests"
  },
  "files": [
    "*.md",
    "LICENSE",
    "docs",
    "lib",
    "config"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "babel --quiet --out-dir lib src",
    "postbuild": "yarn copy-metafiles",
    "copy-metafiles": "node --require @babel/register ./scripts/copyMetafiles",
    "watch": "yarn tests-only --watch",
    "lint": "yarn lint:es && yarn update:eslint-docs --check && yarn lint:md",
    "lint:es": "eslint . --cache",
    "lint:md": "markdownlint \"**/*.md\"",
    "mocha": "cross-env BABEL_ENV=test mocha",
    "test": "yarn build && yarn test-compiled",
    "tests-only": "yarn mocha --compilers js:@babel/register tests/src",
    "test-compiled": "yarn build && BABEL_ENV=testCompiled yarn mocha --compilers js:@babel/register tests/src",
    "prepublishOnly": "yarn build",
    "update:eslint-docs": "yarn build && eslint-doc-generator --rule-doc-title-format prefix-name --rule-doc-section-options false --rule-list-split meta.docs.category --ignore-config stage-0 --config-emoji recommended,☑️"
  },
  "repository": "git+https://github.com/un-es/eslint-plugin-i",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "es6",
    "jsnext",
    "modules",
    "import",
    "export"
  ],
  "author": "JounQin <admin@1stg.me> (https://www.1stG.me)",
  "license": "MIT",
  "devDependencies": {
    "@angular-eslint/template-parser": "^13.5.0",
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/plugin-proposal-decorators": "^7.24.0",
    "@babel/plugin-proposal-export-default-from": "^7.23.3",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-flow": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/register": "^7.23.7",
    "@eslint/import-test-order-redirect-scoped": "link:./tests/files/order-redirect-scoped",
    "@test-scope/some-module": "link:./tests/files/symlinked-module",
    "@typescript-eslint/parser": "^2.23.0 || ^3.3.0 || ^4.29.3 || ^5.10.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "chai": "^4.3.10",
    "cross-env": "^4.0.0",
    "escope": "^3.6.0",
    "eslint": "^2 || ^3 || ^4 || ^5 || ^6 || ^7.2.0 || ^8",
    "eslint-doc-generator": "^1.6.1",
    "eslint-import-resolver-typescript": "^1.0.2 || ^1.1.1",
    "eslint-import-resolver-webpack": "^0.13.8",
    "eslint-import-test-order-redirect": "link:./tests/files/order-redirect",
    "eslint-plugin-eslint-plugin": "^2.3.0",
    "eslint-plugin-import-x": "link:.",
    "eslint-plugin-json": "^2.1.2",
    "fs-copy-file-sync": "^1.1.1",
    "glob": "^7.2.3",
    "jsonc-parser": "=3.2.0",
    "lodash.isarray": "^4.0.0",
    "markdownlint-cli": "~0.35",
    "mocha": "^3.5.3",
    "npm-which": "^3.0.1",
    "redux": "^3.7.2",
    "rimraf": "^2.7.1",
    "sinon": "^2.4.1",
    "typescript": "^2.8.1 || ~3.9.5 || ~4.5.2"
  },
  "peerDependencies": {
    "eslint": "^2 || ^3 || ^4 || ^5 || ^6 || ^7.2.0 || ^8"
  },
  "dependencies": {
    "array-includes": "^3.1.7",
    "array.prototype.findlastindex": "^1.2.4",
    "array.prototype.flat": "^1.3.2",
    "array.prototype.flatmap": "^1.3.2",
    "debug": "^3.2.7",
    "doctrine": "^2.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-module-utils": "^2.8.0",
    "hasown": "^2.0.1",
    "is-core-module": "^2.13.1",
    "is-glob": "^4.0.3",
    "minimatch": "^3.1.2",
    "object.fromentries": "^2.0.7",
    "object.groupby": "^1.0.2",
    "object.values": "^1.1.7",
    "semver": "^6.3.1",
    "tsconfig-paths": "^3.15.0"
  },
  "resolutions": {
    "eslint-module-utils": "link:./utils"
  }
}
